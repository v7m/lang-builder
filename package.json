{
  "name": "lang-builder",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "start": "tsx src/scripts/generate.ts",
    "generate": "tsx src/scripts/generate.ts",
    "main": "tsx src/scripts/generate.ts --main",
    "test": "tsx src/scripts/generate.ts --test",
    "reset": "tsx src/scripts/resetGenerationRegistry.ts",
    "reset-main": "tsx src/scripts/resetGenerationRegistry.ts --main",
    "reset-test": "tsx src/scripts/resetGenerationRegistry.ts --test",
    "reset-all": "npm run reset-main && npm run reset-test",
    "dev": "tsx watch src/scripts/generate.ts",
    "test-mongo": "tsx src/scripts/testMongoConnection.ts"
  },
  "dependencies": {
    "@google/genai": "^0.6.0",
    "@types/mongoose": "^5.11.97",
    "axios": "^1.10.0",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "jsdom": "^26.1.0",
    "mongoose": "^8.16.4",
    "node-fetch": "^3.3.2",
    "p-limit": "^6.2.0",
    "wav": "^1.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@types/ejs": "^3.1.5",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.11.30",
    "@types/wav": "^1.0.4",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "eslint": "^9.30.1",
    "globals": "^16.3.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3",
    "typescript-eslint": "^8.36.0"
  }
}
